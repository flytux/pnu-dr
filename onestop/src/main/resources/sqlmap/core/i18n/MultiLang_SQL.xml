<mapper namespace="core.i18n">
    
    <select id="selectListMultiLang" resultType="java.util.HashMap">
        {CALL SYS.dbo.SYS_공통_다국어_조회()}
    </select>
    
    <select id="insertMultiLang" resultType="java.util.HashMap">
        {CALL SYS.dbo.SYS_공통_다국어_저장(    
            #{SYSTEM_GCD},   
            #{MENU_CD},
            #{KOR_NM},
            #{SESSION_SYSTEM_GCD},
            #{SESSION_MENU_CD},
            #{SESSION_USER_ID},
            #{SESSION_USER_IP}
        )}        
    </select>
  
    
</mapper>
