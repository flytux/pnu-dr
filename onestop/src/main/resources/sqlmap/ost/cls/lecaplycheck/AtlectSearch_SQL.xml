<mapper namespace="ost.cls.lecaplycheck.atlectsearch">

	<select id="searchAutoApplySchdChk" resultType="hashMap">
	    {CALL OST.DBO.OST_수업_수강신청및확인_수강확인_자동신청결과기간_조회(#{SCH_SYEAR}, #{SCH_TERM_GCD}, #{COLL_GRAD_GCD})} 
	</select>
	
	<select id="searchClassOpenSchdChk" resultType="hashMap">
	    {CALL OST.DBO.OST_수업_수강신청및확인_수강확인_개강일자_조회(#{SCH_SYEAR}, #{SCH_TERM_GCD}, #{COLL_GRAD_GCD}, #{STDT_NO})} 
	</select>
	
	<select id="searchAtlectSearchList" resultType="hashMap">
	    {CALL OST.DBO.OST_수업_수강신청및확인_수강확인_수강신청확인_조회(#{SCH_SYEAR}, #{SCH_TERM_GCD}, #{STDT_NO})} 
	</select>
	
	<select id="searchClosedList" resultType="hashMap">
	    {CALL OST.DBO.OST_수업_수강신청및확인_수강확인_폐강강좌현황_조회(#{SCH_SYEAR}, #{SCH_TERM_GCD}, #{STDT_NO})} 
	</select>
	
	<select id="searchCancelList" resultType="hashMap">
	    {CALL OST.DBO.OST_수업_수강신청및확인_수강확인_수강취소현황_조회(#{SCH_SYEAR}, #{SCH_TERM_GCD}, #{STDT_NO})} 
	</select>
	
	<select id="searchAtlectTotSearch" resultType="hashMap">
	    {CALL LECTURE.DBO.CLS_수강신청_수강신청_총신청학점과목수_조회(#{SCH_SYEAR}, #{SCH_TERM_GCD}, 'LECAPPLY', #{STDT_NO}, '0001')} 
	</select>
</mapper>
