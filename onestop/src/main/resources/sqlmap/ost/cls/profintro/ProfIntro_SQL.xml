<mapper namespace="ost.cls.profintro.profintro">

<select id="selectProfIntroGrad" resultType="hashMap">
    { CALL OST.dbo.OST_수업_교수소개_대학원_조회 }
</select>
<select id="selectProfIntroColl" resultType="hashMap">
    {CALL OST.dbo.OST_수업_교수소개_단과대학_조회 }
</select>
<select id="selectProfIntroDept" resultType="hashMap">
    {CALL OST.dbo.OST_수업_교수소개_학과_조회 }
</select>
<select id="selectProfIntro" resultType="hashMap">
    {CALL OST.dbo.OST_수업_교수소개_조회(
    	#{SCH_DEPT_CD},
	    #{FIRSTINDEX},
	    #{LASTINDEX}
    ) }
</select>
<select id="selectProfDetailIntro" resultType="hashMap">
    {CALL OST.dbo.OST_수업_교수소개_상세_조회(
    	#{PROF_NO}
    ) }
</select>


</mapper>
