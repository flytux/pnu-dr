<mapper
	namespace="ost.cls.timetablesearch.examtimetablesearch">

	<select id="selectexamTimetableValid" resultType="hashMap">
		{CALL OST.dbo.OST_수업_시험시간표_납부여부_개강여부_조회(
		#{SCH_SYEAR},
		#{SCH_TERM_GCD},
		#{SESSION_USER_ID}
		)}
	</select>
	<select id="selectexamTimetable" resultType="hashMap">
		{CALL OST.dbo.OST_수업_시험시간표_조회(
		#{SCH_SYEAR},
		#{SCH_TERM_GCD},
		#{SESSION_USER_ID},
		#{SCH_EXAM_GCD}
		)}
	</select>



</mapper>
