<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- * 강의실별 시간표 조회 iBatis SqlMap으로서 작용한다. * @Company : IDINO Co., Ltd. * 
	@Copyright : Copyright (C) by IDINO All rights reserved. * @Author : 김동영 
	* @Create : 2022년 03월 04일 * @Title : SylSeach_SQL * @version 1.0 * @stereotype 
	Sql -->
<mapper
	namespace="ost.cls.timetablesearch.lectroomtimetablesearch">

	<select id="selectLectroomTimetable" resultType="hashMap">
		{CALL OST.dbo.OST_수업_강의실별_시간표_조회(
		#{SCH_LTTM_YN_HID},
		#{SCH_SYEAR},
		#{SCH_TERM_GCD},
		#{SCH_BLD_CD},
		#{SCH_FACILITY_NO}
		)}
	</select>
	<select id="selectbldCd" resultType="hashMap">
		{CALL OST.dbo.OST_수업_강의실별_시간표_조회_건물_조회(
		#{SCH_SYEAR},
		#{SCH_TERM_GCD}
		)}
	</select>
	<select id="selectFacilityNo" resultType="hashMap">
		{CALL OST.dbo.OST_수업_강의실별_시간표_조회_강의실_조회(
		#{SCH_BLD_CD},
		#{SCH_SYEAR},
		#{SCH_TERM_GCD}

		)}
	</select>


</mapper>