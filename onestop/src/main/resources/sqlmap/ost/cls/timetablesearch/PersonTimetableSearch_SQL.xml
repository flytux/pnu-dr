	Sql -->
<mapper
	namespace="ost.cls.timetablesearch.persontimetablesearch">

	<select id="selectPersonTimetableValid" resultType="hashMap">
		{CALL OST.dbo.OST_수업_개인시간표_조회가능여부_조회(
		#{SCH_SYEAR},
		#{SCH_TERM_GCD},
		#{SCH_COLL_GRAD}
		)}
	</select>
	<select id="selectPersonTimetable" resultType="hashMap">
		{CALL OST.dbo.OST_수업_개인시간표_조회(
		#{SCH_LTTM_YN_HID},
		#{SCH_SYEAR},
		#{SCH_TERM_GCD},
		#{SESSION_USER_ID}
		)}
	</select>



</mapper>
