<mapper namespace="ost.cls.syl.sylsearch">

<select id="selectSylSearch" resultType="hashMap">
    {CALL OST.dbo.OST_수업_교수계획표_교수계획표_조회(
	    #{SCH_SYEAR},
	    #{SCH_TERM_GCD},
	    #{SEARCH_GBN},
	    #{profSearch_hidden},
	    #{SCH_SUBJ_NM},
	    #{FIRSTINDEX},
	    #{LASTINDEX}
    )}
</select>

<select id="selectSeachPossDate" resultType="hashMap">
    {CALL OST.dbo.OST_수업_교수계획표_강의평가_조회가능기간_조회(
	    #{SCH_SYEAR},
	    #{SCH_TERM_GCD}
    )}
</select>
</mapper>
