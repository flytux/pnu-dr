<mapper namespace="ost.jolup.hakjukbujolupguide.jolupsupposeInfo">  
    <select id="selectListJolupSupposeInfo" resultType="hashMap">
	    {CALL OST.DBO.OST_졸업_졸업예정정보_조회(
		    #{SESSION_USER_ID},
		    #{SESSION_USER_IP},
		    #{SESSION_SYSTEM_GCD},
		    #{SESSION_MENU_CD}
	    )}
	</select>
	
	<select id="stdtJolupInfo" resultType="hashMap"> 
	    {CALL JOLUP.DBO.JU_학부졸업_졸업대상자관리_학생별졸업예정정보조회_학생조회(
		    #{SESSION_USER_ID}
	    )}
	</select>
	
	
	<select id="hakjukList" resultType="hashMap"> 
	    {CALL OST.DBO.OST_졸업_졸업예정정보_학적신청내역_조회(
		    #{SESSION_USER_ID}
	    )}
	</select>
	
	<select id="subjGbnList" resultType="hashMap"> <!-- 교과목구분별 -->
	    {CALL OST.DBO.OST_졸업_졸업예정정보_교과목구분별이수구분_조회(
		    #{SESSION_USER_ID},
		    #{EVALTN_GCD},
		    #{APPLY_DEPT_CD}
	    )}
	</select>
	
	<select id="mandatorySubjList" resultType="hashMap"> <!-- 필수교과목 -->
	    {CALL JOLUP.DBO.JU_학부졸업_졸업예정정보_필수교과목이수여부_조회1(
		    #{SESSION_USER_ID},
		    #{EVALTN_GCD},
		    #{APPLY_DEPT_CD}
	    )}
	</select>
	
	<select id="cultureChoiceList1" resultType="hashMap"> <!-- 교양선택1 -->
	    {CALL JOLUP.DBO.JU_학부졸업_졸업예정정보_교양선택영역별이수여부_조회1(
		    #{SESSION_USER_ID},
		    #{EVALTN_GCD},
		    #{APPLY_DEPT_CD}
	    )}
	</select>
	
	<select id="cultureChoiceList2" resultType="hashMap"> <!-- 교양선택2 -->
	    {CALL JOLUP.DBO.JU_학부졸업_졸업예정정보_교양선택영역별이수여부_조회2(
		    #{SESSION_USER_ID},
		    #{EVALTN_GCD},
		    #{APPLY_DEPT_CD}
	    )}
	</select>
	
	<select id="cultureChoiceList3" resultType="hashMap"> <!-- 교양선택3 -->
	    {CALL JOLUP.DBO.JU_학부졸업_졸업예정정보_교양선택영역별이수여부_조회3(
		    #{SESSION_USER_ID},
		    #{EVALTN_GCD},
		    #{APPLY_DEPT_CD}
	    )}
	</select>
	
