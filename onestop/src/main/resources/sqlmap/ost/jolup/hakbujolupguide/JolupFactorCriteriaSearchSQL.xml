<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 
 * 졸업요건기준 및 충족여부조회 iBatis SqlMap으로서 작용한다.
 * @Company : IDINO Co., Ltd.
 * @Copyright : Copyright (C) by IDINO All rights reserved.
 * @Author : 김지윤
 * @Create : 2022년 03월 04일
 * @Title : JolupFactorCriteriaSearch_SQL
 * @version 1.0
 * @stereotype Sql
  -->
<mapper namespace="ost.jolup.hakbujolupguide.jolupfactorcriteriasearch">
	
	<select id="selectJolupFactor" parameterType="map" resultType="hashMap">
		{CALL OST.dbo.OST_졸업_졸업요건및충족여부_조회(
			  #{STDT_NO}, #{FIRSTINDEX}, #{LASTINDEX}
			)}
	</select>
	
	<select id="selectJolupStdtInfo" resultType="hashMap">
		{CALL OST.dbo.OST_졸업_졸업요건및충족여부_개인정보_조회(
			  #{STDT_NO}
			)}
	</select>

</mapper>