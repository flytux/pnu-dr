	<select id="selectStdtInfo" resultType="hashMap"> 
	    {CALL HAKJUK.DBO.HJ_학생정보_학적부조회_학적_조회(#{STDT_NO}, #{SESSION_USER_CONTENT})} 
	</select>
	
