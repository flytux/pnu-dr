<mapper namespace="ost.hj.stdtinfo.acctnoupd">   

	<select id="selectAcctNoUpd" resultType="hashMap">
	    {CALL OST.DBO.OST_학적_학적부_계좌번호_조회(#{STDT_NO})} 
	</select>

	<select id="selectBankList" resultType="hashMap">
	    {CALL OST.DBO.OST_학적_학적부_계좌번호_은행목록_조회} 
	</select>

	<select id="saveAcctNoUpd" resultType="hashMap">
	    {CALL OST.DBO.OST_학적_학적부_계좌번호_저장(
	    		#{STDT_NO}
	    	 ,  #{BANK_CD}
	    	 ,  #{ACCT_NO}
	    	 ,  #{ACCT_NM_NM}
	    	 ,  #{SYSTEM_GCD}
	    	 ,  #{CLAUSE_SEQ_NO}
	    	 ,  #{CLAUSE_HISTORY_SEQ_NO}	   
	    	 ,  #{CLAUSE_AGREE_SEQ_NO}	    	 
	    	 ,  #{SESSION_USER_ID}
	    	 ,  #{SESSION_USER_IP}
	    	 ,  #{SESSION_SYSTEM_GCD}
	    	 ,  #{SESSION_MENU_CD}
	    )} 
	</select>
	
	
	<select id="chkAcctNoUpd" resultType="hashMap"> 
	    {CALL OST.DBO.OST_학적_학적부_계좌번호_본인계좌체크(
	    	    #{ACCT_NM_NM}
	    	  , #{STDT_NO}
	     )} 
	</select>


</mapper>
