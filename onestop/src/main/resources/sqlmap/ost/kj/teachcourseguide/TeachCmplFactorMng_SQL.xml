<mapper namespace="ost.kj.teachcourseguide.TeachCmplFactorMng">  

	<select id="selectListTeachCmplFactorMng" resultType="hashMap">
	    {CALL OST.DBO.OST_교직_교직이수요건관리_대상자_조회(#{SESSION_USER_ID},'','')} 
	</select>
	
	<select id="selectListTeachCmplFactorMng2" resultType="hashMap">
	    {CALL OST.DBO.OST_교직_교직이수요건관리_신청내역_조회(#{SESSION_USER_ID},#{PROGRAM_GCD})} 
	</select>
	
	<select id="selectListTeachCmplFactorMng3" resultType="hashMap">
	    {CALL OST.DBO.OST_교직_교직이수요건관리_개설현황_조회(#{SYEAR},#{TERM_GCD},#{PROGRAM_GCD})}
	</select>
	
	<select id="insertTeachCmplFactorMng" resultType="hashMap">
	    {CALL OST.DBO.OST_교직_교직이수요건관리_신청_저장(
    		#{SYEAR},#{TERM_GCD},#{PROGRAM_GCD}, #{PROGRAM_SERIAL_NO}, #{SESSION_USER_ID},
	    	#{SYSTEM_GCD}, #{CLAUSE_SEQ_NO}, #{CLAUSE_HISTORY_SEQ_NO}, #{CLAUSE_AGREE_SEQ_NO},
	    	#{SESSION_USER_ID}, #{SESSION_USER_IP}, #{SESSION_SYSTEM_GCD}, #{SESSION_MENU_CD}
	    )}
	</select>
	
	<select id="deleteTeachCmplFactorMng" resultType="hashMap">
	    {CALL OST.DBO.OST_교직_교직이수요건관리_신청_삭제(
	    	#{SYEAR},#{TERM_GCD},#{PROGRAM_GCD}, #{PROGRAM_SERIAL_NO}, #{SESSION_USER_ID},
	    	#{SESSION_USER_ID}, #{SESSION_USER_IP}, #{SESSION_SYSTEM_GCD}, #{SESSION_MENU_CD}
	    )}
	</select>
	
	<select id="selectListTeachCmplFactorMngPop" resultType="hashMap">
	    {CALL OST.DBO.OST_교직_교직이수요건관리_항목별점수_조회(
	    	#{SYEAR},#{TERM_GCD},#{PROGRAM_GCD}, #{PROGRAM_SERIAL_NO}, #{SESSION_USER_ID}
	    )}
	</select>
	
</mapper>

