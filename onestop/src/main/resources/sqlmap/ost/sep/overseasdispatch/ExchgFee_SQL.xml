<mapper namespace="ost.sep.overseasdispatch.exchgfee">
	
	<select id="searchSchdYearTerm" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_신청가능년도학기_조회(#{EXCHG_GCD}, #{EXCHG_DETAIL_GCD})}
	</select>
	
	<select id="searchSchdChk" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_신청기간_조회(#{SYEAR}, #{TERM_GCD}, #{EXCHG_GCD}, #{EXCHG_DETAIL_GCD}, #{CHASU_NO})}
	</select>
	
	<select id="searchExchgStdtInfo" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_교환교비_신청자정보_조회(#{SYEAR}, #{TERM_GCD}, #{CHASU_NO}, #{SESSION_USER_ID})}
	</select>
	
	<select id="searchDispatchApplyList" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_해외파견프로그램_조회(#{SESSION_USER_ID})}
	</select>
	
	<select id="searchNativeApplyList" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_원어강의참가_조회(#{SESSION_USER_ID})}
	</select>
	
	<select id="searchCollList01" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_지원가능대학목록_조회(#{SYEAR_01}, #{TERM_GCD_01}, #{EXCHG_DETAIL_GCD_01}, #{LANG_GCD_01}, #{DEG_COURSE_GCD_01}, #{CMPL_TERM_CNT_01}, #{CHASU_NO_01})}
	</select>
	
	<select id="searchCollList02" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_지원가능대학목록_조회(#{SYEAR_02}, #{TERM_GCD_02}, #{EXCHG_DETAIL_GCD_02}, #{LANG_GCD_02}, #{DEG_COURSE_GCD_02}, #{CMPL_TERM_CNT_02}, #{CHASU_NO_02})}
	</select>
	
	<select id="searchCollList03" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_지원가능대학목록_조회(#{SYEAR_03}, #{TERM_GCD_03}, #{EXCHG_DETAIL_GCD_03}, #{LANG_GCD_03}, #{DEG_COURSE_GCD_03}, #{CMPL_TERM_CNT_03}, #{CHASU_NO_03})}
	</select>
	
	<select id="saveExchgFee" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_교환교비_저장(
																							#{SYEAR}, 
																							#{TERM_GCD},
																							#{EXCHG_DETAIL_GCD}, 
																							#{SESSION_USER_ID}, 
																							#{STDT_CELLULAR_NO}, 
																							#{STDT_EMAIL},
																							#{EMGCY_TEL_NO},
																							#{PASSPORT_NO},
																							#{PASSPORT_ENG_NM},
																							#{PASSPORT_EXPIRE_DATE},
																							#{PASSPORT_ISSUE_SUPPOSE_DATE},
																							#{FOREIGN_LANGUAGE_1_GCD},
																							#{OFFICIAL_FOREIGN_LANGUAGE_1_GCD},
																							#{ACQ_OFFICIAL_FOREIGN_LANGUAGE_1_SCORE},
																							#{ACQ_OFFICIAL_FOREIGN_LANGUAGE_1_GRADE_GCD},
																							#{FOREIGN_LANGUAGE_2_GCD},
																							#{OFFICIAL_FOREIGN_LANGUAGE_2_GCD},
																							#{ACQ_OFFICIAL_FOREIGN_LANGUAGE_2_SCORE},
																							#{ACQ_OFFICIAL_FOREIGN_LANGUAGE_2_GRADE_GCD},
																							#{FOREIGN_LANGUAGE_3_GCD},
																							#{OFFICIAL_FOREIGN_LANGUAGE_3_GCD},
																							#{ACQ_OFFICIAL_FOREIGN_LANGUAGE_3_SCORE},
																							#{ACQ_OFFICIAL_FOREIGN_LANGUAGE_3_GRADE_GCD},
																							#{APPLY_1_EXUNIV_CD},
																							#{APPLY_1_EXCHG_DETAIL_GCD},
																							#{APPLY_2_EXUNIV_CD},
																							#{APPLY_2_EXCHG_DETAIL_GCD},
																							#{APPLY_3_EXUNIV_CD},
																							#{APPLY_3_EXCHG_DETAIL_GCD},
																							#{CHASU_NO},
																							#{EXCHG_STDT_FRIEND_FG},
																							#{INVITE_PROF_HELP_FG},
																							#{LANG_TRADE_HELP_FG},
																							#{PNU_TOEFL_ATLECT_FG},
																							#{SERVE_ACTIV_PTCPT_TIMESNO},
																							#{SESSION_USER_ID},
																							#{SESSION_USER_IP},
																							#{SESSION_SYSTEM_GCD},
																							#{SESSION_MENU_CD}
																						)}
	</select>
	
	<select id="cancelExchgFee" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_교환교비_신청취소(#{SYEAR}, 
																							  #{TERM_GCD}, 
																							  #{EXCHG_DETAIL_GCD}, 
																							  #{SESSION_USER_ID}, 
																							  #{CHASU_NO},
																							  #{SESSION_USER_ID}, 
																							  #{SESSION_USER_IP},
																							  #{SESSION_SYSTEM_GCD},
																							  #{SESSION_MENU_CD}
																							  )}
	</select>

</mapper>