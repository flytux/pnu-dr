<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 
 * 자비 iBatis SqlMap으로서 작용한다.
 * @Company : IDINO Co., Ltd.
 * @Copyright : Copyright (C) by IDINO All rights reserved.
 * @Author : 최아영
 * @Create : 2022년 02월 03일
 * @Title : OwnExpense_SQL
 * @version 1.0
 * @stereotype Sql
  -->
<mapper namespace="ost.sep.overseasdispatch.ownexpense">
	
	<select id="selectUnivList" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_지원가능대학목록_조회(
			#{SYEAR}
			, #{TERM_GCD}
			, #{EXCHG_DETAIL_GCD}
			, #{LANG_GCD}
			, #{DEG_COURSE_GCD}
			, #{CMPL_TERM_CNT}
			, #{CHASU_NO}
		)}
	</select>
	
	<select id="selectStdtInfo" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_자비_신청자정보(
			#{SYEAR}
			, #{TERM_GCD}
			, #{EXCHG_DETAIL_GCD}
			, #{SESSION_USER_ID}
			, #{CHASU_NO}
		)}
	</select>
	
	<select id="saveOwnExpense" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_자비_입력(
			  #{SYEAR}
			, #{TERM_GCD}
			, #{EXCHG_DETAIL_GCD}
			, #{SESSION_USER_ID}
			, #{CHASU_NO}
			, #{STDT_CELLULAR_NO}
			, #{STDT_EMAIL}
			, #{EMGCY_TEL_NO}
			, #{APPLY_1_EXUNIV_CD}
			, #{APPLY_1_EXUNIV_ETC_NM}
			, #{HYUHAK_EXCHG_FG}
			, #{DISPATCH_SUPPOSE_PERIOD_GCD}
			, #{LANG_GCD}
			, #{SESSION_USER_ID}
			, #{SESSION_USER_IP}
			, #{SESSION_SYSTEM_GCD}
			, #{SESSION_MENU_CD}
		)}
	</select>

	<select id="saveCancelOwnExpense" parameterType="map" resultType="hashMap">
		{CALL OST.DBO.OST_학생교류_해외파견_신청취소(
			  #{SYEAR}
			, #{TERM_GCD}
			, #{EXCHG_DETAIL_GCD}
			, #{SESSION_USER_ID}
			, #{CHASU_NO}
			, #{SESSION_USER_ID}
			, #{SESSION_USER_IP}
			, #{SESSION_SYSTEM_GCD}
			, #{SESSION_MENU_CD}
		)}
	</select>
	
</mapper>