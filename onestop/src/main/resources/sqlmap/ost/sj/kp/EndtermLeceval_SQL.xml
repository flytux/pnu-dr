<mapper namespace="ost.sj.kp.endtermleceval">

	<select id="searchEndSchdChk" resultType="hashMap">
	    {CALL OST.DBO.OST_강의평가_기말강의평가_학사일정_조회(
	    #{SYEAR},
	    #{TERM_GCD}
	    )} 
	</select>
	
	<select id="selectEndtermLeceval" resultType="hashMap">
	    {CALL OST.DBO.OST_강의평가_기말강의평가_조회(
	    #{SYEAR},
	    #{TERM_GCD},
	    #{SESSION_USER_ID},
	    #{FIRSTINDEX},
	    #{LASTINDEX}
	    )} 
	</select>
	
	<select id="selectEndtermLecevalQuestionList" resultType="hashMap">
	    {CALL OST.DBO.OST_강의평가_기말강의평가_평가_전체문항_조회(
	    #{SYEAR},
	    #{TERM_GCD},
	    #{SUBJ_NO},
	    #{CLASS_NO},
	    #{CHARGE_PROF_NO},
	    #{LECEVAL_GCD},
	    #{SESSION_USER_ID}
	    )} 
	</select>
	
	<select id="saveEndtermLecevalQuestion" resultType="hashMap">
    {CALL OST.DBO.OST_강의평가_기말강의평가_평가_저장(
	    #{SYEAR},
	    #{TERM_GCD},
	    #{SUBJ_NO},
	    #{CLASS_NO},
	    #{SESSION_USER_ID},
	    #{LECEVAL_GCD},
	    #{CHARGE_PROF_NO},
	    #{QUESTION_NO},
	    #{LECEVAL_QUESTION_NO},
	    #{radioGroup},
	    #{multipleTxt},
	   	#{SESSION_USER_ID},
	    #{SESSION_USER_IP},
	    #{SESSION_SYSTEM_GCD},
	    #{SESSION_MENU_CD}
	    )} 
	</select>


	<select id="selectEduCertLecevalQuestionList" resultType="hashMap">
	    {CALL OST.DBO.OST_강의평가_기말강의평가_교육인증_문항_조회(
	    #{SYEAR},
	    #{TERM_GCD},
	    #{SUBJ_NO},
	    #{CLASS_NO},
	    #{CHARGE_PROF_NO},
	    #{EDUCERT_GCD},
	    #{EDUCERT_PROGRAM_GCD},
	    #{QUESTION_GROUP_NO}
	    )}  
	</select> 
	
	
	<select id="saveEduCertLecevalQuestion" resultType="hashMap">
	    {CALL OST.DBO.OST_강의평가_기말강의평가_교육인증_평가_저장(
	    #{SYEAR},
	    #{TERM_GCD},
	    #{SUBJ_NO},
	    #{CLASS_NO},
	    #{SESSION_USER_ID},
	    #{CHARGE_PROF_NO},
	    #{QUESTION_NO},
	    #{SUB_QUESTION_NO},
	    #{REPLY_VALUE},
	    #{multipleTxt},
	   	#{SESSION_USER_ID},
	    #{SESSION_USER_IP},
	    #{SESSION_SYSTEM_GCD},
	    #{SESSION_MENU_CD}
	    )} 
	</select> 
</mapper>
