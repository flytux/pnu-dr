<mapper namespace="common.sample">

<select id="sample1SelectList" resultType="hashMap">
    {CALL SYS.DBO.SYS_공통코드관리_샘플_조회(
    #{SCH_SYSTEM_GCD},
    '',
    #{SCH_COMMON_CD},
    #{SCH_COMMON_CD_KOR_NM},
    '',
    '',
    #{FIRSTINDEX},
    #{LASTINDEX})} 
</select>

<select id="executeSampleInfo" resultType="hashMap">
	{ CALL SYS.DBO.SYS_공통코드관리_샘플_저장(
        #{rowState},
        #{SYSTEM_GCD},
        #{COMMON_CD},
        #{COMMON_CD_KOR_NM},
        #{USE_FG},
        #{SESSION_USER_ID},
        #{SESSION_USER_IP},
        #{SESSION_SYSTEM_GCD},
        #{SESSION_MENU_CD}
    )}
</select>

</mapper>
